<RelativeLayout
    android:name="college.edu.tomer.parselogin.GalleryActivityFragment"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    >


    <FrameLayout
        android:layout_width="match_parent"
        android:background="#ffa4cb"
        android:layout_height="200dp" android:id="@+id/frame1"/>

    <FrameLayout
        android:layout_below="@+id/frame1"
        android:layout_width="match_parent"
        android:layout_height="200dp" android:id="@+id/frame2"/>

    <FrameLayout
        android:layout_below="@+id/frame2"
        android:background="#fadf"
        android:layout_width="match_parent"
        android:layout_height="200dp" android:id="@+id/frame3"/>
</RelativeLayout>
